{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@auth/*": [
        "src/modules/auth/*"
      ],
      "@counter/*": [
        "src/modules/counter/*"
      ],
      "@users/*": [
        "src/modules/users/*"
      ]
    }
  }
}
